{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Node 12+es2020",
   
    "compilerOptions": {
      "lib": ["es2020", "es2020.promise", "es2020.bigint", "es2020.string"],
      "module": "es2020",
      "target": "es2020",
      "outDir": "dist",
      "sourceMap": true,
   
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    }  
  }